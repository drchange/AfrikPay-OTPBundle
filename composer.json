{
    "name": "afrikpay/otp-bundle",
    "type": "symfony-bundle",
    "description": "OTP SMS Bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Dr Change",
            "email": "trsor.simo.dev@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.2",
        "symfony/framework-bundle": "^3.4|^4.0|^5.0",
        "doctrine/doctrine-bundle": "~1.4|~2.0",
        "doctrine/orm": "~2.4,>=2.4.5"
    },
    "autoload": {
        "psr-4" : {
            "AfrikPay\\OTPBundle\\" : ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": { "Cron\\CronBundle\\Tests\\": "Tests/" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^5.0",
        "symfony/symfony": "^4.0|^5.0"
    }
}


